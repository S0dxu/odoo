{
  "manifest_version": 3,
  "name": "Odoo",
  "version": "1.0",
  "permissions": [],
  "host_permissions": [
    "https://221e.odoo.com/odoo/action-215/*"
  ],
  "content_scripts": [
    {
      "matches": [ "https://221e.odoo.com/odoo/action-215/*", "https://221e.odoo.com/odoo/crm/*", "https://221e.odoo.com/odoo/action-219/*" ],
      "js": [ "content.js" ],
      "run_at": "document_idle"
    }
  ],
  "action": {
    "default_icon": {
      "128": "odoo.png"
    }
  }
}
